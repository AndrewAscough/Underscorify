#include <iostream> 
#include <string> 
int main   (  )    {   std :: string  s1 = "hello  world" ;  
 std :: cout << "s1  was:  "   <<   s1 <<   std :: endl ;  
char c   = '!' ;  
 std :: cout << "s1+c  =  "   <<  s1   <<   c <<   std :: endl ;  
  	 s1 = "onewordwithsymbols<<3 /*7&&|";
	std::cout<<s1<<std::endl;
	return 0;
}
